    "@discordjs/rest": "^0.3.0",
    "better-sqlite3": "^7.5.0",
    "discord.js": "^13.6.0",
    "enmap": "^5.8.8",
    "eslint-plugin-jsdoc": "^38.1.6",
    "lodash": "^4.17.21",
    "node-fetch": "^3.2.3",
    "node-schedule": "^2.1.0",
    "pg": "^8.7.3",
    "tmi.js": "^1.8.5",
    "winston": "^3.6.0",
    "winston-daily-rotate-file": "^4.6.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.180",
